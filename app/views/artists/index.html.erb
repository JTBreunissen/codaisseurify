<div class="row">
  <div class="col-md-9">
    <div class="panel panel-primary">
      <div class="panel-heading">
        All artists
      </div>
      <div class="panel-body">

        <% @artist.each do |artist| %>
        <div class="row">
          <div class="col-md-7">
            <h4><%= link_to artist.artistname, artist %></h4>
          </div>
        </div>
        <% end %>

      </div>
    </div>

  <%= link_to "Add new artist", new_artist_path, class: "btn btn-primary" %>
  </div>
</div>
